{
  "pixela_api": {
     "request_header": {
        "X-USER-TOKEN": ""
      },
    "create_user": {
      "method": "POST",
      "endpoint": "https://pixe.la/v1/users",
      "request_body": {
        "token": "",
        "username": "",
        "agreeTermsOfService": "",
        "notMinor": "",
        "thanksCode": ""
      }
    },
    "create_graph": {
      "method": "POST",
      "endpoint": "https://pixe.la/v1/users/<username>/graphs",

      "request_body": {
        "id": "",
        "name": "",
        "unit": "",
        "type": "",
        "color": "",
        "timezone": ""
      }
    },
    "post_pixel": {
      "method": "POST",
      "endpoint": "https://pixe.la/v1/users/<username>/graphs/<graphID>",
      "request_body": {
        "date": "",
        "quantity": "",
        "optionalData": ""
      }
    },
    "update_pixel": {
      "method": "PUT",
      "endpoint": "https://pixe.la/v1/users/<username>/graphs/<graphID>/<yyyyMMdd>",
      "request_header": {
        "X-USER-TOKEN": ""
      },
      "request_body": {
        "quantity": "",
        "optionalData": ""
      }
    }
  }
}